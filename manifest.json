{
  "manifest_version": 2,
  "name": "Mind Matter",
  "description": "Blocks distracting sites, granting access only if a trivia question is answered correctly.",
  "version": "0.4.2",
  "version_name": "0.4.2 alpha",
  "minimum_chrome_version": "51",
  "author": "Jennings Zhang",
  "background": {
    "scripts": [
      "ext/js/sqi.js",
      "ext/js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "ext/js/sqi.js",
        "ext/js/app.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": "assets/brain-in-pot16.png",
    "default_title": "Mind Matter",
    "default_popup": "ext/ui/popup.html"
  },
  "options_page": "ext/ui/options",
  "options_ui": {
    "page": "ext/ui/options/index.html",
    "chrome_style": false
  },
  "icons": {
    "16": "assets/brain-in-pot16.png",
    "32": "assets/brain-in-pot32.png",
    "64": "assets/brain-in-pot64.png",
    "128": "assets/brain-in-pot128.png"
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "optional_permissions": [
    "notifications"
  ]
}