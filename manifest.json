{
  "manifest_version": 2,
  "name": "Mind Matters",
  "description": "An educational chrome extension, not a waste of time. Prompts the user with a multiple-choice question popup before visiting a distracting site.",
  "version": "0.2",
  "author": "Jennings Zhang",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icons/brain-in-pot.png",
    "default_title": "Mind Matter",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/brain-in-pot16.png",
    "24": "icons/brain-in-pot24.png",
    "32": "icons/brain-in-pot32.png",
    "48": "icons/brain-in-pot42.png",
    "96": "icons/brain-in-pot96.png",
    "256": "icons/brain-in-pot256.png"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "https://jennydaman.github.io/mindmatter/"
  ]
}
